    function UnfunctionalPart(){
        alert('這部分功能尚未開放，敬請期待！');
    }
    function ChangePlan(id){
        if(currentplan!=id){
            $('#'+currentplan).toggleClass('select');
            currentplan = id;
            $('#'+currentplan).toggleClass('select');
            //change left
            //splice id to A,B...
            var id_slice = id.slice(0,1);
            list = FindCorrectList(id_slice);
            if(list!=''){
                currentid = list[0].productCode;
                WriteProductBlock(list);
                WriteProductInformation(list[0]);
            }else{
                document.getElementById('productInformationInner').innerHTML = '';
                document.getElementById('productBlock').innerHTML = '';
                alert('很抱歉，此價格區間並沒有符合您需求的飼料\n\n卡卡貓建議您可以查看其他價格區間。');
            }
        }else{
            //do nothing
        }
    }
    function ChangeProduct(id){
        if(currentid!=id){
            $("#productInformation").animate({scrollTop:0}, 400);   
            $('#'+currentid).toggleClass('product_select');
            currentid = id;
            $('#'+currentid).toggleClass('product_select');
            //change right
            for(i=0;i<list.length;i++){
                if(list[i].productCode==currentid){
                    WriteProductInformation(list[i]);
                    break;
                }
            }
            
        }else{
            //do nothing
        }
    }
    function Send(){
        $.ajax({
            method: 'POST',
            url: 'saveQuestionnaire_result',
            data:{
                currentProduct: currentid
            }
        }).done(function(){
            window.location.href = 'purchase';
        });
    }
