    function ChangePlan(id){
        if(currentplan!=id){
            $('#'+currentplan).toggleClass('select');
            currentplan = id;
            $('#'+currentplan).toggleClass('select');
            //change left
            //splice id to A,B...
            var id_splice = id.splice(0,1);
            list = FindCorrectList(id_splice);
            currentid = list[0].productCode;
            WriteProductBlock(list);
            WriteProductInformation(list[0]);
        }else{
            //do nothing
        }
    }
    function ChangeProduct(id){
        if(currentid!=id){
            $("#productInformation").animate({scrollTop:0}, 400);   
            $('#'+currentid).toggleClass('product_select');
            currentid = id;
            $('#'+currentid).toggleClass('product_select');
            //change right
            for(i=0;i<list.length;i++){
                if(list[i].productCode==currentid){
                    WriteProductInformation(list[i]);
                    break;
                }
            }
            
        }else{
            //do nothing
        }
    }
    function Send(){
        $.ajax({
            method: 'POST',
            url: 'saveQuestionnaire_result',
            data:{
                currentProduct: currentid
            }
        }).done(function(){
            window.location.href = 'purchase_confirm';
        });
    }
