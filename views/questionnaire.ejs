	<script>
		var customBasicEnum = {
			boy:"男",
			girl:"女",
			_18_:"18歲以下",
			_18_25:"18-25",
			_26_35:"26-35",
			_36_45:"36-45",
			_46_65:"46-65",
			_65_:"65歲以上",
			yes:"是",
			no:"否",
			highFreq:"高",
			medFreq:"中",
			lowFreq:"低",
			inside:"室內",
			outside:"室外",
			medside:"一半一半",
			tooThin:"過瘦",
			aLittleThin:"稍瘦",
			medium:"適中",
			aLittleFat:"稍胖",
			tooFat:"過胖"
		}
		var basicInformation = {
			name:"",
			catName:"",
			sex:"",
			catSex:"",
			age:"",
			catAge:"",
			species:"",
			ligation:"",
			pregnancy:"無資料",
			catWeight:"",
			catExerciseFreq:"",
			catSide:"無資料",
			catSize:""
		}		
		//
		function enterPress(id,event){
			var x = event.which || event.keyCode;
			var y = id.slice(0,11) + "Button";
			if(x==13)  document.getElementById(y).click();
		}
		function addToList(id){
			//questionA01
			if(id=="questionA01Button"){
				if($("#questionA01Enter").val()!=""){
					basicInformation.name = $("#questionA01Enter").val();
					$("#questionA01").fadeOut(500,function(){
						$("#questionA02").fadeIn(500,function(){
							$("#questionA02Enter").focus();
						});
					});
				}
			}
			//questionA02
			if(id=="questionA02Button"){
				if($("#questionA02Enter").val()!=""){
					basicInformation.catName = $("#questionA02Enter").val();
					for(i=1;i<8;i++){
						var str = "catName" + i;
						document.getElementById(str).innerHTML = basicInformation.catName;
					}
					$("#questionA02").fadeOut(500,function(){
						$("#questionA03").fadeIn(500);
					});
				}
			}
			//questionA03
			if(id=="questionA03_girl"){
				basicInformation.sex = customBasicEnum.girl;
				$("#questionA03").fadeOut(500,function(){
					$("#questionA04").fadeIn(500);
				});
			}
			if(id=="questionA03_boy"){
				basicInformation.sex = customBasicEnum.boy;
				$("#questionA03").fadeOut(500,function(){
					$("#questionA04").fadeIn(500);
				});
			}
			//questionA04
			if(id=="questionA04_girl"){
				basicInformation.catSex = customBasicEnum.girl;
				$("#questionA04").fadeOut(500,function(){
					$("#questionA05").fadeIn(500);
				});
			}
			if(id=="questionA04_boy"){
				basicInformation.catSex = customBasicEnum.boy;
				$("#questionA04").fadeOut(500,function(){
					$("#questionA05").fadeIn(500);
				});
			}
			//questionA05
			if(id=="questionA05_18-"){
				basicInformation.age = customBasicEnum._18_;
				$("#questionA05").fadeOut(500,function(){
					$("#questionA06").fadeIn(500);
				});
			}
			if(id=="questionA05_18-25"){
				basicInformation.age = customBasicEnum._18_25;
				$("#questionA05").fadeOut(500,function(){
					$("#questionA06").fadeIn(500);
				});
			}
			if(id=="questionA05_26-35"){
				basicInformation.age = customBasicEnum._26_35;
				$("#questionA05").fadeOut(500,function(){
					$("#questionA06").fadeIn(500);
				});
			}
			if(id=="questionA05_36-45"){
				basicInformation.age = customBasicEnum._36_45;
				$("#questionA05").fadeOut(500,function(){
					$("#questionA06").fadeIn(500);
				});
			}
			if(id=="questionA05_46-65"){
				basicInformation.age = customBasicEnum._46_65;
				$("#questionA05").fadeOut(500,function(){
					$("#questionA06").fadeIn(500);
				});
			}
			if(id=="questionA05_65+"){
				basicInformation.age = customBasicEnum._65_;
				$("#questionA05").fadeOut(500,function(){
					$("#questionA06").fadeIn(500,function(){
						$("#questionA06Enter").focus();
					});
				});
			}
			//questionA06
			if(id=="questionA06Button"){
				if($("#questionA06Enter").val()!=""){
					basicInformation.catAge = $("#questionA06Enter").val();
					$("#questionA06").fadeOut(500,function(){
						$("#questionA07").fadeIn(500);	
					});
				}
			}
			//questionA07
			if(id=="questionA07Button"){
				if($("#questionA07_nohair").val()!="0"){
					basicInformation.species = $("#questionA07_nohair").val();
				}
				if($("#questionA07_shorthair").val()!="0"){
					basicInformation.species = $("#questionA07_shorthair").val();
				}
				if($("#questionA07_longhair").val()!="0"){
					basicInformation.species = $("#questionA07_longhair").val();
				}
				$("#questionA07").fadeOut(500,function(){
					$("#questionA08").fadeIn(500);	
				});
			}
			//questionA08
			if(id=="questionA08_yes"){
				basicInformation.ligation = customBasicEnum.yes;
				$("#questionA08").fadeOut(500,function(){
					$("#questionA10").fadeIn(500);	
				});
			}
			if(id=="questionA08_no"){
				basicInformation.ligation = customBasicEnum.no;
				if(basicInformation.catSex==customBasicEnum.girl){
					$("#questionA08").fadeOut(500,function(){
						$("#questionA09").fadeIn(500);	
					});
				}else{
					$("#questionA08").fadeOut(500,function(){
						$("#questionA10").fadeIn(500);	
					});
				}
			}
			//questionA09
			if(id=="questionA09_yes"){
				basicInformation.pregnancy = customBasicEnum.yes;
				$("#questionA09").fadeOut(500,function(){
					$("#questionA10").fadeIn(500);	
				});
			}
			if(id=="questionA09_no"){
				basicInformation.pregnancy = customBasicEnum.no;
				$("#questionA09").fadeOut(500,function(){
					$("#questionA10").fadeIn(500);	
				});
			}
			//questionA10
			if(id=="questionA10Button"){
				if($("#questionA10Enter").val()!=""){
					basicInformation.catWeight = $("#questionA10Enter").val();
					$("#questionA10").fadeOut(500,function(){
						$("#questionA11").fadeIn(500);	
					});
				}
			}
			//questionA11
			if(id=="questionA11_highFreq"){
				basicInformation.catExerciseFreq = customBasicEnum.highFreq;
				$("#questionA11").fadeOut(500,function(){
					$("#questionA12").fadeIn(500);	
				});
			}
			if(id=="questionA11_medFreq"){
				basicInformation.catExerciseFreq = customBasicEnum.medFreq;
				$("#questionA11").fadeOut(500,function(){
					$("#questionA13").fadeIn(500);	
				});
			}
			if(id=="questionA11_lowFreq"){
				basicInformation.catExerciseFreq = customBasicEnum.lowFreq;
				$("#questionA11").fadeOut(500,function(){
					$("#questionA13").fadeIn(500);	
				});
			}
			//questionA12
			if(id=="questionA12_inside"){
				basicInformation.catSide = customBasicEnum.inside;
				$("#questionA12").fadeOut(500,function(){
					$("#questionA13").fadeIn(500);	
				});
			}
			if(id=="questionA12_outside"){
				basicInformation.catSide = customBasicEnum.outside;
				$("#questionA12").fadeOut(500,function(){
					$("#questionA13").fadeIn(500);	
				});
			}
			if(id=="questionA12_medside"){
				basicInformation.catSide = customBasicEnum.medside;
				$("#questionA12").fadeOut(500,function(){
					$("#questionA13").fadeIn(500);	
				});
			}
			//questionA13
			if(id=="questionA13_tooThin"){
				basicInformation.catSize = customBasicEnum.tooThin;
				$("#questionA13").fadeOut(500,function(){
					showResult();
				});
			}
			if(id=="questionA13_aLittleThin"){
				basicInformation.catSize = customBasicEnum.aLittleThin;
				$("#questionA13").fadeOut(500,function(){
					showResult();
				});
			}
			if(id=="questionA13_medium"){
				basicInformation.catSize = customBasicEnum.medium;
				$("#questionA13").fadeOut(500,function(){
					showResult();
				});
			}
			if(id=="questionA13_aLittleFat"){
				basicInformation.catSize = customBasicEnum.aLittleFat;
				$("#questionA13").fadeOut(500,function(){
					showResult();
				});
			}
			if(id=="questionA13_tooFat"){
				basicInformation.catSize = customBasicEnum.tooFat;
				$("#questionA13").fadeOut(500,function(){
					showResult();
				});
			}
		}
		function speciesCheck(id){
			if(id=="questionA07_nohair"){
				$("#questionA07_shorthair").val("0");
				$("#questionA07_longhair").val("0");
			}
			if(id=="questionA07_shorthair"){
				$("#questionA07_nohair").val("0");
				$("#questionA07_longhair").val("0");
			}
			if(id=="questionA07_longhair"){
				$("#questionA07_nohair").val("0");
				$("#questionA07_shorthair").val("0");
			}
		}
		function showResult(){
			var resultInformation = "";
			resultInformation += "<h3>飼主姓名 : " + basicInformation.name + "</h3>";
			resultInformation += "<h3>貓貓姓名 : " + basicInformation.catName + "</h3>";
			resultInformation += "<h3>飼主性別 : " + basicInformation.sex + "</h3>";
			resultInformation += "<h3>貓貓性別 : " + basicInformation.catSex + "</h3>";
			resultInformation += "<h3>飼主年齡 : " + basicInformation.age + "</h3>";
			resultInformation += "<h3>貓貓年齡 : " + basicInformation.catAge + "</h3>";
			resultInformation += "<h3>貓貓品種 : " + basicInformation.species + "</h3>";
			resultInformation += "<h3>貓貓結紮 : " + basicInformation.ligation + "</h3>";
			resultInformation += "<h3>貓貓懷孕 : " + basicInformation.pregnancy + "</h3>";
			resultInformation += "<h3>貓貓體重 : " + basicInformation.catWeight + "</h3>";
			resultInformation += "<h3>貓貓活動 : " + basicInformation.catExerciseFreq + "</h3>";
			resultInformation += "<h3>室內室外 : " + basicInformation.catSide + "</h3>";
			resultInformation += "<h3>貓貓體型 : " + basicInformation.catSize + "</h3>";
			document.getElementById("result").innerHTML = resultInformation;
		}
	</script>
</head>
