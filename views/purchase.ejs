	var storeData = [];
	var currentStore = 'null';
	function FindStore(){
		if($('#cityName').val()!='縣、市' && $('#townName').val()!='區、鎮、鄉')
		$.ajax({
			method : 'POST',
			url : '/clientAskFor711',
			data : {
				cityName : $('#cityName').val(),
				townName : $('#townName').val()
			}
		}).done(function(data){
			storeData = JSON.parse(data);
			document.getElementById('selector_search').value = '';
			var innerHTML = '';
			innerHTML += '<table class="table table-hover"><thead><tr><th></th><th>店號</th><th>門市名稱</th><th>門市地址</th></tr></thead><tbody>';
			for(i=0;i<storeData.length;i++){
				innerHTML += '<tr><th><a id="'+storeData[i].storeName+'" onclick="ChangeStore(this.id)">選擇此門市</a></th><th>'+storeData[i].storeID+'</th><th>'+storeData[i].storeName+'</th><th>'+storeData[i].storeAddress+'</th></tr>';
			}			
			innerHTML += '</tbody></table>';
			document.getElementById('storeData').innerHTML = innerHTML;
			$('.storeSelector').fadeIn(500);
		});
	}
	function ChangeStore(id){
		currentStore = id;
		document.getElementById('currentStore').innerHTML = ' - '+id+'門市';
		$('.storeSelector').fadeOut(500);
	}
	function SearchStore(){
		var condition = $('#selector_search').val();
		var innerHTML = '';
		innerHTML += '<table class="table table-hover"><thead><tr><th></th><th>店號</th><th>門市名稱</th><th>門市地址</th></tr></thead><tbody>';
		for(i=0;i<storeData.length;i++){
			if(storeData[i].storeID.includes(condition) || storeData[i].storeName.includes(condition) || storeData[i].storeAddress.includes(condition))
			innerHTML += '<tr><th><a id="'+storeData[i].storeName+'" onclick="ChangeStore(this.id)">選擇此門市</a></th><th>'+storeData[i].storeID+'</th><th>'+storeData[i].storeName+'</th><th>'+storeData[i].storeAddress+'</th></tr>';
		}			
		innerHTML += '</tbody></table>';
		document.getElementById('storeData').innerHTML = innerHTML;
	}
	function SendOrder(){
		if(delivery=='delivery_A' && $('#username').val()!='' && $('#userphone').val()!='' && $('#userhomeaddress').val()!=''){
			$.ajax({
				method : 'POST',
				url : '/clientSendOrder',
				data : {
					username : $('#username').val(),
					useraddress : $('#userhomeaddress').val(),
					userphone : $('#userphone').val(),
					logistics : 'logisticsA',
					productCode : purchaseItem.productCode,
					productName_ZH : purchaseItem.productName_ZH
				}
